{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mew",
      "camelcase": "Mew",
      "title": "MEW",
      "scope": "source.mew",
      "file-types": ["imew", "mew", "wgsl"],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^(mew|wgsl|imew)$",
      "locals": "queries/locals.scm",
      "class-name": "TreeSitterMew"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Modular Extensions to WGSL (MEW) grammar to tree-sitter",
    "authors": [
      {
        "name": "Natalie Cuthbert",
        "email": "natalie@cuthbert.co.za",
        "url": "https://github.com/ncthbrt"
      },
      {
        "name": "Mathis Brossier",
        "email": "mathis.brossier@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/ncthbrt/tree-sitter-mew"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": false,
    "python": false,
    "rust": true,
    "swift": false,
    "zig": false
  }
}
